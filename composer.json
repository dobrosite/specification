{
    "name": "dobrosite/specification",
    "description": "Specification pattern for PHP",
    "type": "library",
    "keywords": [
        "spec",
        "specification"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Михаил Красильников",
            "email": "m.krasilniov@dobro.site"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "platform": {
            "php": "5.6.40"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "DobroSite\\Specification\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DobroSite\\Specification\\Tests\\": "tests"
        }
    }
}